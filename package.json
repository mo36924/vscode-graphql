{
  "name": "vscode-graphql-tag",
  "displayName": "GraphQL",
  "version": "0.0.2",
  "private": true,
  "description": "",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "graphql"
  ],
  "homepage": "https://github.com/mo36924/vscode-graphql#readme",
  "bugs": {
    "url": "https://github.com/mo36924/vscode-graphql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mo36924/vscode-graphql.git"
  },
  "license": "MIT",
  "author": "mo36924",
  "publisher": "mo36924",
  "scripts": {
    "format": "run-s format:*",
    "format:sort-package-json": "sort-package-json",
    "format:prettier": "prettier --write .",
    "publish": "run-s format publish:*",
    "publish:vsce": "vsce publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.js.jsx",
          "source.tsx",
          "source.vue",
          "source.svelte"
        ],
        "scopeName": "inline.graphql",
        "path": "./grammars/graphql.js.json",
        "embeddedLanguages": {
          "meta.embedded.block.graphql": "graphql"
        }
      }
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "sort-package-json": "^1.55.0",
    "vsce": "^2.7.0"
  },
  "engines": {
    "vscode": "^1.66.0"
  }
}
